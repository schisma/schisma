{
  "name": "schisma_gui",
  "version": "0.0.1",
  "description": "A GUI for Schisma",
  "private": true,
  "scripts": {
    "list_midi_inputs": "node ./bin/list_midi_inputs",
    "dev": "nodemon --exec npm run start",
    "start": "node ./bin/www",
    "dev:css": "nodemon --exec npm run start:css",
    "start:css": "npm run build:css && node ./bin/www",
    "build:css": "postcss public/stylesheets/source.css -o public/stylesheets/style.css"
  },
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "cookie-parser": "~1.4.4",
    "csv-parser": "^2.3.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "feather-icons": "^4.28.0",
    "find-process": "^1.4.4",
    "fkill": "^7.0.1",
    "handsontable": "^8.1.0",
    "highcharts": "^8.2.0",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.20",
    "midi": "^1.0.0",
    "morgan": "~1.9.1",
    "papaparse": "^5.3.0",
    "postcss": "^8.2.1",
    "pug": "^3.0.0",
    "tailwindcss": "^2.0.2",
    "untildify": "^4.0.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "postcss-cli": "^8.3.1"
  }
}
